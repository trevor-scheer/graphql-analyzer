{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "emitDeclarationOnly": true,
    "declarationDir": "../out-e2e/.declarations"
  },
  "include": ["./**/*.ts"],
  "references": [{ "path": ".." }]
}
